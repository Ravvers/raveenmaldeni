		"typescript": "^4.9.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "npm run lint && npm run build:nolint",
		"build:nolint": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"prettier": "prettier --config .prettierrc src/**/*.{ts,tsx,css} --write",
		"depcheck": "npx depcheck",
		"lint": "eslint --max-warnings 0"
	},
	"eslintConfig": {
		"root": true,
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"prettier"
		],
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"rules": {
			"prettier/prettier": 2
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
